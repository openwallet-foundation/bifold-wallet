{
  "name": "aries-bifold",
  "version": "0.0.1",
  "scripts": {
    "style-watch": "onchange '**/*.js' -- prettier --write {{changed}}",
    "style-check": "prettier --check '**/*.js'",
    "style-format": "prettier --write '**/*.js'",
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "npx react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.0",
    "aries-mobileagent-react-native": "file:aries-mobileagent-react-native",
    "axios": "^0.21.0",
    "patch-package": "^6.4.4",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-camera": "^3.31.1",
    "react-native-fs": "^2.16.6",
    "react-native-get-random-values": "^1.4.0",
    "react-native-keychain": "^6.2.0",
    "react-native-qrcode-svg": "^6.0.6",
    "react-native-svg": "^12.1.0",
    "react-router-native": "^5.2.0",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "commitlint": "^11.0.0",
    "eslint": "^7.12.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.63.0",
    "prettier": "^2.1.2",
    "react-test-renderer": "16.9.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "preset": "react-native",
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-router-dom|react-router-native)/"
    ]
  }
}
